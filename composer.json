{
    "name": "achillesp/matryoshka",
    "description": "Russian Doll Caching for Laravel.",
    "license": "MIT",
    "keywords": ["laravel", "cache"],
    "authors": [
        {
            "name": "Achilles Panagiotou",
            "email": "achilles.p@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php" : "^7.0",
        "illuminate/support": "^5.5",
        "illuminate/config": "^5.5"
    },
    "autoload": {
        "psr-4": {
            "Achillesp\\Matryoshka\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Achillesp\\Matryoshka\\Test\\": "tests"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^6.3",
        "orchestra/testbench": "~3.0"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Achillesp\\Matryoshka\\MatryoshkaServiceProvider"
            ]
        }
    }
}
